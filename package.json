{
  "name": "express-sequelize-ts",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn src/app.ts",
    "dev:pm": "tsc && pm2 restart sequil",
    "build": "tsc",
    "sequelize": "npx sequelize --config ./sequelize.config.js",
    "migration:create": "npx sequelize migration:create --name",
    "migration:generate": "npx sequelize migration:generate --name",
    "migration:run": "npx sequelize db:migrate --config ./sequelize.config.js",
    "migration:undo": "npx sequelize db:migrate:undo --config ./sequelize.config.js"
  },
  "dependencies": {
    "@types/aws-serverless-express": "^3.3.9",
    "aws-sdk": "^2.1691.0",
    "aws-serverless-express": "^3.4.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.13.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.4",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
